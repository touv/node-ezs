{
    "name": "ezs",
    "version": "8.1.2",
    "description": "A wrapper to build Stream transformers with functional style",
    "directories": {
        "test": "test"
    },
    "main": "./lib/index.js",
    "bin": {
        "ezs": "./bin/ezs"
    },
    "scripts": {
        "test": "mocha --require babel-register --exit",
        "coverage": "nyc --reporter=text mocha --reporter dot --require babel-register --exit",
        "doc": "documentation readme package.json src/* --section=Statements",
        "lint": "eslint --ext=.js ./test/*.js ./src/*.js ./src/statements/*.js",
        "build": "babel src --out-dir lib",
        "prepublish": "npm run build",
        "postversion": "git push && git push --tags",
        "#pretest": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/touv/node-ezs.git"
    },
    "keywords": [
        "stream",
        "transformer"
    ],
    "author": "Nicolas Thouvenin <nthouvenin@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/touv/node-ezs/issues"
    },
    "homepage": "https://github.com/touv/node-ezs#readme",
    "devDependencies": {
        "babel-cli": "~6.26.0",
        "babel-eslint": "~10.0.1",
        "babel-preset-env": "~1.7.0",
        "babel-preset-stage-3": "~6.24.1",
        "babel-register": "~6.26.0",
        "documentation": "~8.1.2",
        "eslint": "~5.14.1",
        "eslint-config-airbnb-base": "~13.1.0",
        "eslint-plugin-import": "~2.16.0",
        "from": "~0.1.7",
        "mocha": "~6.0.2",
        "node-fetch": "~2.3.0",
        "nyc": "~13.3.0"
    },
    "dependencies": {
        "app-module-path": "~2.2.0",
        "autocast": "~0.0.4",
        "debug": "~4.1.1",
        "deep-object-diff": "~1.1.0",
        "filename-regex": "~2.0.1",
        "filtrex": "~0.5.4",
        "http-shutdown": "~1.2.0",
        "keyv-lru-files": "~1.0.2",
        "lodash": "~4.17.11",
        "merge2": "~1.2.3",
        "mkdirp": "~0.5.1",
        "node-dir": "~0.1.17",
        "node-object-hash": "~1.4.2",
        "notevil": "~1.1.0",
        "pumpify": "~1.5.1",
        "yargs": "~13.2.1"
    },
    "optionalDependencies": {}
}
